      </ImageView>
      <ImageView fx:id="player1" fitHeight="53.0" fitWidth="99.0" layoutX="68.0" layoutY="457.0" pickOnBounds="true">
         <image>
            <Image url="@player_1.png" />
         </image>
      </ImageView>
      <ImageView fx:id="player2" fitHeight="53.0" fitWidth="99.0" layoutX="198.0" layoutY="455.0" pickOnBounds="true">
         <image>
            <Image url="@player_2.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="33.0" fitWidth="40.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true">
         <image>
            <Image url="@return.png" />
         </image>
      </ImageView>
      <Button fx:id="returnbutton" layoutX="16.0" layoutY="18.0" mnemonicParsing="false" onAction="#onReturnClick" opacity="0.0" prefHeight="26.0" prefWidth="40.0" />
   </children>
</AnchorPane>
